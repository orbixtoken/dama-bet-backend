#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Mauricio/Projetos/Dama_Bet/backend/node_modules/z-schema/bin/node_modules:/mnt/c/Users/Mauricio/Projetos/Dama_Bet/backend/node_modules/z-schema/node_modules:/mnt/c/Users/Mauricio/Projetos/Dama_Bet/backend/node_modules:/mnt/c/Users/Mauricio/Projetos/Dama_Bet/node_modules:/mnt/c/Users/Mauricio/Projetos/node_modules:/mnt/c/Users/Mauricio/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Mauricio/Projetos/Dama_Bet/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Mauricio/Projetos/Dama_Bet/backend/node_modules/z-schema/bin/node_modules:/mnt/c/Users/Mauricio/Projetos/Dama_Bet/backend/node_modules/z-schema/node_modules:/mnt/c/Users/Mauricio/Projetos/Dama_Bet/backend/node_modules:/mnt/c/Users/Mauricio/Projetos/Dama_Bet/node_modules:/mnt/c/Users/Mauricio/Projetos/node_modules:/mnt/c/Users/Mauricio/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Mauricio/Projetos/Dama_Bet/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../z-schema/bin/z-schema" "$@"
else
  exec node  "$basedir/../z-schema/bin/z-schema" "$@"
fi
